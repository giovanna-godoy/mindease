{
  "version": 2,
  "buildCommand": "npm run build:prod",
  "outputDirectory": "dist/apps/shell",
  "framework": null,
  "installCommand": "npm install",
  "devCommand": "npx nx serve shell",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}
